<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yashodainfotech.queuingapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" /> <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
/>
    <application
        android:name=".Myapplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_allnewnew"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_allnewnew_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity android:name=".GuestActivity" />
        <activity android:name=".TableActivity" />
        <activity android:name=".DemoActivity" />
        <activity android:name=".Toast" />
        <activity android:name=".DashboardActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".AdminActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".CameraActivityGuest" />

        <receiver android:name=".MyBroadcastReceiver" />
        <receiver android:name=".MyBroadcastReceivernew" />
        <receiver android:name=".MyBroadcastReceiverVideo" />
    </application>

</manifest>